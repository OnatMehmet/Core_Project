
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="tr">

@await Html.PartialAsync("/Views/Admin/PartialHead.cshtml")
<body>
    <div class="container-scroller">
        <!-- partial:../../partials/_sidebar.html -->
        @await Html.PartialAsync("/Views/Admin/PartialSideBar.cshtml")
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:../../partials/_navbar.html -->
            @await Html.PartialAsync("/Views/Admin/PartialNavBar.cshtml")
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper ">
                    @await Html.PartialAsync("/Views/Admin/PartialHeader.cshtml")
                   
                        @RenderBody()
                    
                </div>
                <!-- content-wrapper ends -->

                <!-- partial:../../partials/_footer.html -->
                @await Html.PartialAsync("/Views/Admin/PartialFooter.cshtml")
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    @await Html.PartialAsync("/Views/Admin/PartialScript.cshtml")
</body>
</html>
